{
  "name": "express-typescript-backend",
  "version": "1.0.0",
  "description": "Backend project with Node.js, Express and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc -p tsconfig.json",
    "seed": "ts-node src/scripts/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo Build completed successfully!",
    "test-db": "ts-node src/test-db-connection.ts",
    "backup-db": "ts-node src/scripts/backup-db.ts",
    "migrate-db": "ts-node src/scripts/migrate-db.ts",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@types/axios": "^0.9.36",
    "@types/qrcode": "^1.5.6",
    "@vercel/functions": "^3.3.6",
    "@vercel/node": "^5.5.28",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "glob": "^13.0.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.21.1",
    "node-thermal-printer": "^4.5.0",
    "qrcode": "^1.5.4",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "rimraf": "^6.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "overrides": {
    "rimraf": "^6.1.2",
    "glob": "^13.0.0"
  }
}

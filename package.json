{
  "name": "express-typescript-backend",
  "version": "1.0.0",
  "description": "Backend project with Node.js, Express and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev:backend": "ts-node-dev --respawn src/server.ts",
    "dev:frontend": "vite --config vite.config.ts",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:client": "vite --config vite.config.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build --config vite.config.ts",
    "build:backend": "tsc",
    "build:full": "npm install && npm run build",
    "seed": "ts-node src/scripts/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo Build completed successfully!"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@hookform/resolvers": "^3.3.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.91.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "mongoose": "^8.21.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "ws": "^8.19.0",
    "yup": "^1.3.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.21",
    "vitest": "^2.1.8",
    "wait-on": "^9.0.3"
  }
}